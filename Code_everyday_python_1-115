def all_unique(s):
    # A set automatically stores only unique elements
    seen = set()
    for char in s:
        if char in seen:
            return False  # duplicate found
        seen.add(char)
    return True


# --- Testing ---
print(all_unique("abcde"))   # True (all unique)
print(all_unique("Hello"))   # False ('l' repeats)
print(all_unique("Python"))  # True
print(all_unique("Aa"))      # True (case-sensitive)
